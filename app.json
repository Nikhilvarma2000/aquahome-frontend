{
  "expo": {
    "name": "AquaHome",
    "slug": "aquahome",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "@/assets/icons/aqua_home.png",
    "splash": {
      "image": "@/assets/icons/aqua_home.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aquahome.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aquahome.app",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow AquaHome to use your location to find nearby service areas."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b1ff345b-5580-4efa-9593-9ba8fef5999e"
      }
    }
  }
}
